        if (recognizing) {
            recognition.stop();
            $('#form_save').show();
            $('#reset').show();
            reset();
        } else {
            recognition.start();
            recognizing = true;
            button.innerHTML = "Kliknij, aby zatrzymać";
            $('#button').removeClass('btn-success').addClass('btn-danger');
        }
    }

